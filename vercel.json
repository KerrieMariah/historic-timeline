{
  "rewrites": [
    {
      "source": "/",
      "destination": "/hk-vessels-museum-homepage.html"
    },
    {
      "source": "/so-fung",
      "destination": "/so-fung-timeline.html"
    },
    {
      "source": "/so-fung-light",
      "destination": "/so-fung-timeline-light.html"
    },
    {
      "source": "/star-ferry",
      "destination": "/star-ferry-timeline-elegant.html"
    },
    {
      "source": "/star-ferry-ultimate",
      "destination": "/star-ferry-timeline-ultimate.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}

